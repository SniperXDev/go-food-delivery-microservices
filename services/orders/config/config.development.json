{
  "serviceName": "order_service",
  "deliveryType": "http",
  "context": {
    "timeout": 20
  },
  "grpc": {
    "port": ":6005",
    "development": true
  },
  "http": {
    "port": ":8000",
    "development": true,
    "timeout": 30,
    "basePath": "/api/v1",
    "host": "http://localhost",
    "ordersPath": "orders",
    "debugHeaders": true,
    "httpClientDebug": true,
    "debugErrorsResponse": true,
    "ignoreLogUrls": [
      "metrics"
    ]
  },
  "probes": {
    "readinessPath": "/ready",
    "livenessPath": "/live",
    "port": ":3002",
    "pprof": ":6002",
    "prometheusPath": "/metrics",
    "prometheusPort": ":8002",
    "checkIntervalSeconds": 10
  },
  "logger": {
    "level": "debug",
    "devMode": false,
    "encoder": "json"
  },
  "postgres": {
    "host": "localhost",
    "port": 5432,
    "user": "postgres",
    "password": "postgres",
    "dbName": "orders_service",
    "sslMode": false,
    "migrations": {
      "migrationsDir": "migrations",
      "skipMigration": true
    }
  },
  "gormPostgres": {
    "host": "localhost",
    "port": 5432,
    "user": "postgres",
    "password": "postgres",
    "dbName": "orders_service",
    "sslMode": false
  },
  "kafka": {
    "brokers": [
      "localhost:9092"
    ],
    "groupID": "orders_service_consumer",
    "initTopics": true
  },
  "kafkaTopics": {
    "orderCreate": {
      "topicName": "order_create",
      "partitions": 10,
      "replicationFactor": 1
    },
    "orderCreated": {
      "topicName": "order_created",
      "partitions": 10,
      "replicationFactor": 1
    }
  },
  "jaeger": {
    "enable": true,
    "serviceName": "orders_service",
    "hostPort": "localhost:6831",
    "logSpans": false
  },
  "eventStoreConfig": {
    "connectionString": "esdb://localhost:2113?tls=false"
  }
}